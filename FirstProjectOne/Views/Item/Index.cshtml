@using FirstProjectOne.Models
@model IEnumerable <Itemlist>
@{
    ViewBag.Title = "Index";

}

<h2>Index</h2>
<h1>@ViewBag.shopName</h1>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
            <th>Category</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var data in Model)
        {
            <tr>
                <td>@data.id</td>
                <td>@data.name</td>
                <td>@data.price</td>
                <td>@data.category</td>
            </tr>
        }
    </tbody>
</table>

